   <VBox alignment="CENTER" spacing="40.0">
      <Button fx:id="continueButton" text="Wznów" mnemonicParsing="false"/>
      <Button fx:id="restartButton" text="Zacznij od nowa" mnemonicParsing="false"/>
      <Button fx:id="quitButton" text="Wyjdź" mnemonicParsing="false"/>
   </VBox>
</StackPane>
